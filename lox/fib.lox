fun fib_r(n) {
    if (n < 2) return n;
    return fib_r(n - 2) + fib_r(n - 1);
}

fun fib_i(n) {
    if (n < 1) {
        return 0;
    }
    var a = 0;
    var b = 1;
    for (var i = 0; i < n-1; i = i + 1) {
        var c = b;
        b = a + b;
        a = c;
    }
    return b;
}

print "fib recursive:";
var start = clock();
print fib_r(35);
print clock() - start;

print "fib iter:";
start = clock();
print fib_i(35);
print clock() - start;
